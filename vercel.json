{
  "buildCommand": "pnpm build",
  "devCommand": "pnpm dev",
  "installCommand": "pnpm install",
  "framework": "astro",
  "outputDirectory": "dist",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, s-maxage=60, stale-while-revalidate=86400"
        }
      ]
    }
  ],
  "env": {
    "CONTENTFUL_SPACE_ID": "@contentful_space_id",
    "CONTENTFUL_PREVIEW_TOKEN": "@contentful_preview_token",
    "CONTENTFUL_DELIVERY_TOKEN": "@contentful_delivery_token",
    "CONTENTFUL_WEBHOOK_SECRET": "@contentful_webhook_secret"
  }
}
